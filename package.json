{
  "name": "FORMOCR",
  "version": "1.0.0",
  "description": "File Recognizer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./Images/icon.icns --prune=true --out=../release-mac",
    "package-win": "electron-packager . form-recognizer --overwrite --platform=win32 --arch=x64 --icon=./Images/icon.ico --prune=true --out=../release-win",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=./Images/icon.icns --prune=true --out=../release-linux"
  },
  "author": "Sayak Kundu",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^5.0.1",
    "fix-path": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-packager": "^14.2.1"
  }
}
